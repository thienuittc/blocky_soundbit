<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category name="CAR" colour="#000000" icon="road">
    <block type="block_led"/>
    <block type="block_led_car"/>
    <block type="block_single_led">
    <value name="number_led"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
    <value name="color"><shadow type="colour_picker"><field name="COLOUR">#ff0000</field></shadow></value>
    </block>
    <block type="block_car_forward">
    <value name="speed"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
    </block>
    <block type="block_car_backward">
    <value name="speed"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
    </block>
    <block type="block_car_left">
    <value name="speed"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
    </block>
    <block type="block_car_right">
    <value name="speed"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
    </block>
    <block type="block_line_sensor">
    <value name="num"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
    </block>
    <block type="block_control_motor">
    <value name="speed"><shadow type="math_number"><field name="NUM">0</field></shadow></value>
    </block>
    <block type="block_car_stop"/>
    <block type="block_ultrasonic_checkdistance"/>
    <block type="block_checkchar"/>
    <block type="block_mes_bluetooth"/>
  </category>
</xml>